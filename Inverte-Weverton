#Criar um programa em Assembly que inverta a ordem dos elementos de um vetor de tamanho n.
#Vetor
.data
c: .word 1,2,3,4,5,6
d: .word 

.text
la $s0, c  #endereço base do vetor
la $s1, d
addi $s2,$zero,0
addi $t0, $zero, 10

loop:
	add $t1, $zero, $s0  # pimeira posição de A
	add $t2, $zero, $s1  # primeira posição de B
	lw $t3, 0($t1) #pega o valor na posição de memória
	subi $s2, $s2, 6 # percorre 6 posição do fim para o inicio de A
	
	sw $t3, 0($t4) #pega o valor na posição de memória
	addi $t0, $t0, 6 #percorre 6 posição do inicio para o fim de B
	slt $t5,$t0, $zero  #compara se $t0 não esta menor que 0
	bne $t5, 1, loop # se for diferente continua
